{
  "name": "sms-sender-fullstack",
  "version": "1.0.0",
  "description": "SMS Sender Full Stack Application",
  "scripts": {
    "build": "echo 'Build completed - deploy backend and frontend separately'",
    "dev": "echo 'Use individual dev commands for backend and frontend'",
    "backend:dev": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:install": "cd backend && pip install -r requirements.txt",
    "frontend:install": "cd frontend && npm install",
    "frontend:build": "cd frontend && npm run build"
  },
  "keywords": ["sms", "fastapi", "nextjs", "mongodb"],
  "author": "Md Mostafizur Rahaman",
  "license": "MIT"
}